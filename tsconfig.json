{
  "compilerOptions": {
    "rootDir": "src",
    "outDir": "dist",
    "module": "esnext",
    "target": "ESNext",
    "lib": [
      "es6",
      "dom",
      "es2016",
      "es2017",
      "ESNext"
    ],
    "sourceMap": true,
    "allowJs": false,
    "jsx": "react-jsx",
    "jsxImportSource": "woby",
    "declaration": true,
    "declarationDir": "./dist/types",
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    // "noImplicitReturns": true,
    // "noImplicitThis": true,
    // "noImplicitAny": true,
    // "strictNullChecks": false,
    // "noUnusedLocals": true,
    // "noUnusedParameters": true
  },
  // "include": [
  //   "src"
  // ],
  "exclude": [
    "node_modules",
    "dist",
    "example",
    "vite.*.*ts",
  ]
}